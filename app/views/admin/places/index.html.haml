-filters = params.slice('filter')
.manager
  .top.clearfix
    .pagination
      -unless @places.last_page?
        .next=link_to "Next &rarr;".html_safe, admin_places_path(filters.merge(:page => @places.current_page + 1))
      -unless @places.first_page?
        .previous=link_to "&larr; Previous".html_safe, admin_places_path(filters.merge(:page => @places.current_page - 1))
    %h1.title="#{@places.total_count} Places"
    .links
      =link_to "Image Queue", admin_places_path(filters.merge(:filter => 3))
      =link_to "Imageless", admin_places_path(filters.merge(:filter => 2))
      =link_to "Wishlisted", admin_places_path(filters.merge(:filter => 1))
      =link_to "Processing", admin_places_path(filters.merge(:filter => 4))
      =link_to "All", admin_places_path
      =link_to "New", new_admin_place_path
  =render :partial => "table", :object => @places, :as => :places
  .pagination.bottom
    -unless @places.last_page?
      .next=link_to "Next &rarr;".html_safe, admin_places_path(params.slice('filter').merge(:page => @places.current_page + 1))
    -unless @places.first_page?
      .previous=link_to "&larr; Previous".html_safe, admin_places_path(params.slice('filter').merge(:page => @places.current_page - 1))
