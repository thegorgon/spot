.image_search_results
  -@images.each do |title, images|
    .section
      %h1=title
      -images.each do |image|
        =form_for [:admin, @place], :html => {"data-place-id" => @place.id} do |f|
          =f.hidden_field :external_image_url, :value => image.url
          =f.fields_for :image_attribution do |ia|
            =ia.hidden_field :source, :value => image.source
            =ia.hidden_field :owner_url, :value => image.owner_url
          .image_potential{:class => image.source, :style => "background-image: url(#{image.url(:size => 'm')});"}